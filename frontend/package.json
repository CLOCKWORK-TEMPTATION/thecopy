{
  "name": "nextn",
  "version": "1.2.0",
  "description": "النسخة - Landing Page مع Video Text Mask و Cards Scanner",
  "engines": {
    "node": "20.x",
    "pnpm": ">=10.0.0"
  },
  "private": true,
  "scripts": {
    "dev": "next dev -p 5000 -H 0.0.0.0",
    "dev:fallback": "node --max-old-space-size=4096 scripts/dev-with-fallback.js",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "prebuild": "node scripts/generate-pages-manifest.js",
    "build": "cross-env NODE_ENV=production next build",
    "build:production": "NODE_ENV=production ESLINT_NO_DEV_ERRORS=true next build",
    "start": "next start -p 5000 -H 0.0.0.0",
    "analyze": "ANALYZE=true pnpm build",
    "lint": "eslint \"src/app/(main)/directors-studio/page.tsx\" \"src/app/(main)/directors-studio/helpers/projectSummary.ts\" \"src/app/(main)/directors-studio/components/LoadingSection.tsx\" \"src/app/(main)/directors-studio/components/NoProjectSection.tsx\" \"src/app/(main)/directors-studio/components/PageLayout.tsx\" \"src/app/(main)/directors-studio/components/ProjectContent.tsx\" \"src/app/(main)/directors-studio/components/ProjectTabs.tsx\" --no-warn-ignored --max-warnings=10",
    "lint:fix": "eslint \"src/app/(main)/directors-studio/page.tsx\" \"src/app/(main)/directors-studio/helpers/projectSummary.ts\" \"src/app/(main)/directors-studio/components/LoadingSection.tsx\" \"src/app/(main)/directors-studio/components/NoProjectSection.tsx\" \"src/app/(main)/directors-studio/components/PageLayout.tsx\" \"src/app/(main)/directors-studio/components/ProjectContent.tsx\" \"src/app/(main)/directors-studio/components/ProjectTabs.tsx\" --no-warn-ignored --fix --max-warnings=10",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "test": "vitest run \"src/app/(main)/directors-studio/helpers/__tests__/projectSummary.test.ts\"",
    "test:smoke": "vitest run \"src/app/__smoke__/simple.test.ts\"",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "test:all": "pnpm test:coverage && pnpm e2e",
    "performance:report": "node scripts/performance-report.js",
    "a11y:ci": "playwright test --grep @a11y",
    "perf:ci": "playwright test --grep @performance",
    "lighthouse": "lhci autorun",
    "lighthouse:collect": "lhci collect",
    "lighthouse:assert": "lhci assert",
    "lighthouse:upload": "lhci upload",
    "budget:check": "node scripts/check-performance-budget.js",
    "budget:report": "pnpm build && pnpm budget:check",
    "perf:analyze": "node scripts/performance-optimization.js",
    "perf:full": "pnpm build && pnpm budget:report && pnpm perf:analyze",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org $SENTRY_ORG --project $SENTRY_PROJECT .next && sentry-cli sourcemaps upload --org $SENTRY_ORG --project $SENTRY_PROJECT .next",
    "approve-builds": "echo 'Frontend build warnings approved - OpenTelemetry and Sentry warnings are expected and safe' && node -e 'console.log(\"✅ Build approved with expected warnings\")';",
    "prepush": "pnpm lint && pnpm typecheck && pnpm test",
    "ci": "pnpm lint && pnpm typecheck && pnpm test && pnpm build && pnpm e2e"
  },
